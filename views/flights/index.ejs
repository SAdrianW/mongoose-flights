<%- include('../partials/header') %>

<h2>All Flight List</h2>

<ul>
    <% flights.forEach(flight => { %>
        <li>
            <%= flight.airline %>
            -
            <%= flight.airport %>
            -
            <%= flight.flightNo %>
            -
            <%= flight.departs %>

        </li>

    <% }); %>
</ul>


